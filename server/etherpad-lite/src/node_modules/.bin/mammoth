#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/.versions/mammoth@1.8.0/bin/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/.versions/mammoth@1.8.0/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/.versions/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/node_modules:/home/salma/Documentos/initp/server/node_modules:/home/salma/Documentos/initp/node_modules:/home/salma/Documentos/node_modules:/home/salma/node_modules:/home/node_modules:/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/.versions/mammoth@1.8.0/bin/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/.versions/mammoth@1.8.0/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/.versions/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/plugin_packages/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/src/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/node_modules:/home/salma/Documentos/initp/server/node_modules:/home/salma/Documentos/initp/node_modules:/home/salma/Documentos/node_modules:/home/salma/node_modules:/home/node_modules:/node_modules:/home/salma/Documentos/initp/server/etherpad-lite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mammoth/bin/mammoth" "$@"
else
  exec node  "$basedir/../mammoth/bin/mammoth" "$@"
fi
