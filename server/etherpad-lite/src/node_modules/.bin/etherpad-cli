#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/salma/Documentos/initp/server/server/etherpad-lite/node_modules/.pnpm/etherpad-cli-client@3.0.2/node_modules/etherpad-cli-client/node_modules:/home/salma/Documentos/initp/server/server/etherpad-lite/node_modules/.pnpm/etherpad-cli-client@3.0.2/node_modules:/home/salma/Documentos/initp/server/server/etherpad-lite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/salma/Documentos/initp/server/server/etherpad-lite/node_modules/.pnpm/etherpad-cli-client@3.0.2/node_modules/etherpad-cli-client/node_modules:/home/salma/Documentos/initp/server/server/etherpad-lite/node_modules/.pnpm/etherpad-cli-client@3.0.2/node_modules:/home/salma/Documentos/initp/server/server/etherpad-lite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../etherpad-cli-client/cli.js" "$@"
else
  exec node  "$basedir/../etherpad-cli-client/cli.js" "$@"
fi
