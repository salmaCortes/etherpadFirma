require.define({"ep_headings2/static/js/shared.js": {"(module ep_headings2/static/js/shared.js)": function (require, exports, module) {"use strict";const tags=["h1","h2","h3","h4","code"];exports.collectContentPre=(o,t,s)=>{const e=t.tname,n=t.state.lineAttributes,a=tags.indexOf(e);return(e==="div"||e==="p")&&delete n.heading,a>=0&&(n.heading=tags[a]),s()},exports.collectContentPost=(o,t,s)=>{const e=t.tname,n=t.state.lineAttributes;return tags.indexOf(e)>=0&&delete n.heading,s()};
