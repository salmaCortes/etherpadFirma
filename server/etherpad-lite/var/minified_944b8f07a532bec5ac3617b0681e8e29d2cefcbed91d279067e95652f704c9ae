require.define({"ep_copy_paste_select_all/static/js/copy_paste_select_all.js": {"(module ep_copy_paste_select_all/static/js/copy_paste_select_all.js)": function (require, exports, module) {"use strict";exports.postAceInit=(t,i)=>{$("#selectAll").click(()=>{i.ace.callWithAce(n=>{const o=n.ace_getDocument(),c=$(o).find("body").contents().length;n.ace_performSelectionChange([0,0],[c-1,0],!1)},"selectAll",!0)}),$("#findAndReplace").click(()=>{const n=prompt("Search for..."),o=prompt("Replace with..."),c=$('iframe[name="ace_outer"]').contents().find("iframe").contents().find("#innerdocbody").children("div");$(c).each(function(){findAndReplace(n,o,this)})})};const findAndReplace=(t,i,n)=>{if(!t||typeof i>"u")return;const o=typeof t=="string"?new RegExp(t,"gi"):t,c=(n||document.body).childNodes;let r=c.length;const a=["html","head","style","title","meta","script","object","iframe","link"];for(;r--;){const e=c[r];if(e.nodeType===1&&a.indexOf(e.nodeName.toLowerCase()===-1)&&findAndReplace(t,i,e),e.nodeType!==3||!o.test(e.data))continue;const s=e.parentNode,f=(()=>{const m=e.data.replace(o,i),d=document.createElement("div"),l=document.createDocumentFragment();for(d.innerHTML=m;d.firstChild;)l.appendChild(d.firstChild);return l})();s.insertBefore(f,e),s.removeChild(e)}};
}}["(module ep_copy_paste_select_all/static/js/copy_paste_select_all.js)"],
});
