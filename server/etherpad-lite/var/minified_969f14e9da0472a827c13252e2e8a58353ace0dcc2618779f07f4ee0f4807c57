require.define({"ep_set_title_on_pad/static/js/index.js": {"(module ep_set_title_on_pad/static/js/index.js)": function (require, exports, module) {"use strict";exports.handleClientMessage_CUSTOM=(i,e,l)=>{if(e.payload.action==="recieveTitleMessage"){const t=e.payload.message,a=$("#input_title"),s=$("#title > h1 > a");a.is(":visible")||t&&(window.document.title=t,s.text(t),s.removeAttr("data-l10n-id"),a.val(t),clientVars.ep_set_title_on_pad={},clientVars.ep_set_title_on_pad.title=t)}l(null)};const sendTitle=()=>{const i=pad.getUserId(),e=pad.getPadId(),t={type:"title",action:"sendTitleMessage",message:$("#input_title").val(),padId:e,myAuthorId:i};pad.collabClient.sendMessage(t)};exports.documentReady=()=>{$("#options-title").click(()=>{$("#pad_title").toggleClass("display_important"),$("#pad_title").toggleClass("flex_title")}),clientVars.ep_set_title_on_pad||($("#title > h1 > a").text(clientVars.padId),$("#title > h1 > a").removeAttr("data-l10n-id")),$("#editorcontainerbox").hasClass("flex-layout")||$.gritter.add({title:"Error",text:"ep_set_title_on_pad: Upgrade to etherpad 1.8.3+ for this plugin to work correctly",sticky:!0,class_name:"error"}),$("#edit_title").show(),$("#edit_title").click(()=>{$("#input_title, #save_title").show(),$("#title, #edit_title").hide(),$("#input_title").focus()}),$("#save_title").click(()=>{sendTitle(),window.document.title=$("#input_title").val(),$("#title > h1 > a").text($("#input_title").val()),$("#title > h1 > a").removeAttr("data-l10n-id"),$("#title, #edit_title").show(),$("#input_title, #save_title").hide()});function i(e,l=300){let t;return(...a)=>{clearTimeout(t),t=setTimeout(()=>{e.apply(this,a)},l)}}$("#input_title").keyup(e=>{i(sendTitle),window.document.title=$("#input_title").val(),$("#title > h1").text($("#input_title").val()),e.keyCode===13&&$("#save_title").click()})};
