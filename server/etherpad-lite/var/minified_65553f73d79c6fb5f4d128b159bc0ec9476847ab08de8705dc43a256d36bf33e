"use strict";const bodyStyles=window.getComputedStyle(document.body),normal={};normal.lightcolor=bodyStyles.getPropertyValue("--light-color"),normal.superdarkcolor=bodyStyles.getPropertyValue("--super-dark-color"),normal.darkcolor=bodyStyles.getPropertyValue("--dark-color"),normal.primarycolor=bodyStyles.getPropertyValue("--primary-color"),normal.middlecolor=bodyStyles.getPropertyValue("--middle-color"),normal.superlightcolor=bodyStyles.getPropertyValue("--super-light-color"),normal.textcolor=bodyStyles.getPropertyValue("--text-color");const themes={change:()=>{themes.setThemeByName($("#themesmenu").val()),require("ep_etherpad-lite/static/js/pad_cookie").padcookie.setPref("themeName",$("#themesmenu").val())},setTheme:(e,o,r,t,i,a,s)=>{document.body.style.setProperty("--light-color",e),document.body.style.setProperty("--super-dark-color",o),document.body.style.setProperty("--dark-color",r),document.body.style.setProperty("--primary-color",t),document.body.style.setProperty("--middle-color",i),document.body.style.setProperty("--text-color",a),document.body.style.setProperty("--super-light-color",s);const l=$('iframe[name="ace_outer"]').contents().find("body").get(0).style;l.setProperty("--primary-color",t),l.setProperty("--super-light-color",s),l.setProperty("--super-dark-color",o),l.setProperty("--light-color",e),l.setProperty("--dark-color",r);const c=$('iframe[name="ace_outer"]').contents().find("iframe").contents().find("body").get(0).style;c.setProperty("--super-dark-color",o),c.setProperty("--primary-color",t)},init:()=>{const e=require("ep_etherpad-lite/static/js/pad_cookie").padcookie;let o=themes.getUrlVars().theme;o||(e.getPref("themeName")&&(o=e.getPref("themeName")),!o&&clientVars.theme_default&&(o=clientVars.theme_default)),o&&themes.setThemeByName(o)},setThemeByName:e=>{e==="normal"&&themes.setTheme(normal.lightcolor,normal.superdarkcolor,normal.darkcolor,normal.primarycolor,normal.middlecolor,normal.textcolor,normal.superlightcolor),e==="highcontrast"&&