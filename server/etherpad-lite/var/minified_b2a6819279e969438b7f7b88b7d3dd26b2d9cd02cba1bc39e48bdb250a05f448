require.define({"ep_subscript_and_superscript/static/js/index.js": {"(module ep_subscript_and_superscript/static/js/index.js)": function (require, exports, module) {"use strict";exports.postAceInit=(s,e)=>{$(".subscript").click(()=>{e.ace.callWithAce(t=>{t.ace_getAttributeOnSelection("sub")?t.ace_setAttributeOnSelection("sub",!1):t.ace_setAttributeOnSelection("sub",!0)},"insertsubscript",!0)}),$(".superscript").click(()=>{e.ace.callWithAce(t=>{t.ace_getAttributeOnSelection("sup")?t.ace_setAttributeOnSelection("sup",!1):t.ace_setAttributeOnSelection("sup",!0)},"insertsuperscript",!0)})},exports.aceEditEvent=(s,{callstack:e,editorInfo:t})=>{if(e.type!=="handleClick"&&e.type!=="handleKeyEvent"&&!e.docTextChanged)return!1;setTimeout(()=>{t.ace_getAttributeOnSelection("sub")?$(".subscript > a").addClass("activeButton"):$(".subscript > a").removeClass("activeButton"),t.ace_getAttributeOnSelection("sup")?$(".superscript > a").addClass("activeButton"):$(".superscript > a").removeClass("activeButton")},250)},exports.aceAttribsToClasses=(s,e)=>{if(e.key==="sup"||e.key==="sub")return[e.key]},exports.aceRegisterBlockElements=()=>["sub","sup"],exports.aceAttribClasses=(s,e)=>(e.sub="tag:sub",e.sup="tag:sup",e);
}}["(module ep_subscript_and_superscript/static/js/index.js)"],
});
