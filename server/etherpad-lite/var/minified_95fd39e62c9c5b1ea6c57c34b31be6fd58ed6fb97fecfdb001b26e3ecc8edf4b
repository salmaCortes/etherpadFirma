"use strict";(n=>{let e=n.cookie.match(/language=((\w{2,3})(-\w+)?)/);e&&(e=e[1]),html10n.bind("indexed",()=>{html10n.localize([e,navigator.language,navigator.userLanguage,"en"])}),html10n.bind("localized",()=>{n.documentElement.lang=html10n.getLanguage(),n.documentElement.dir=html10n.getDirection()})})(document);
