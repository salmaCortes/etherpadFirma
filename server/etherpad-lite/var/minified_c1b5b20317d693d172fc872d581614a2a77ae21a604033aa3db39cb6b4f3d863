"use strict";// @license magnet:?xt=urn:btih:8e4f440f4c65981c5bf93c76d35135ba5064d8b7&dn=apache-2.0.txt Apache-2.0
(()=>{const o=window.onerror;window.onerror=(...d)=>{const[m,i,u,s,c]=d;document.body.textContent="";for(const h of document.querySelectorAll('head style, head link[rel="stylesheet"]'))h.remove();const e=document.body;e.textContent="";const l=document.createElement("p");e.appendChild(l),l.appendChild(document.createTextNode("An error occurred while loading the page:"));const n=document.createElement("blockquote");e.appendChild(n),n.style.fontWeight="bold",n.appendChild(document.createTextNode(m));const t=document.createElement("p");e.appendChild(t),t.appendChild(document.createTextNode(`in ${i}`)),t.appendChild(document.createElement("br")),t.appendChild(document.createTextNode(`at line ${u}:${s}`));const a=document.createElement("p");e.appendChild(a),a.appendChild(document.createTextNode("Stack trace:"));const r=document.createElement("blockquote");e.appendChild(r);const p=document.createElement("pre");r.appendChild(p),p.appendChild(document.createTextNode(c.stack||c.toString())),typeof o=="function"&&o(...d)}})();// @license-end
