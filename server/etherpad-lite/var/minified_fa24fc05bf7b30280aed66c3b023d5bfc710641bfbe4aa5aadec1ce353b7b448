require.define({"ep_comments_page/static/js/commentBoxes.js": {"(module ep_comments_page/static/js/commentBoxes.js)": function (require, exports, module) {"use strict";let padOuter;const getPadOuter=()=>(padOuter=padOuter||$('iframe[name="ace_outer"]').contents(),padOuter),getCommentsContainer=()=>getPadOuter().find("#comments"),hideComment=(e,n)=>{const t=getCommentsContainer().find(`#${e}`);t.removeClass("full-display"),n&&t.hide(),$('iframe[name="ace_outer"]').contents().find('iframe[name="ace_inner"]').contents().find("head .comment-style").remove(),getPadOuter().find(".comment-modal").removeClass("popup-show")},hideAllComments=()=>{getCommentsContainer().find(".sidebar-comment").removeClass("full-display"),getPadOuter().find(".comment-modal").removeClass("popup-show")},highlightComment=(e,n,t)=>{const o=getCommentsContainer(),c=o.find(`#${e}`),l=$('iframe[name="ace_outer"]').contents().find('iframe[name="ace_inner"]');if(o.is(":visible"))o.find(".sidebar-comment").each(function(){l.contents().find("head .comment-style").remove(),$(this).removeClass("full-display")}),c.addClass("full-display"),l.contents().find("head").append(`<style class='comment-style'>.${e}{ color: #a7680c !important }</style>`);else{const m=c.clone(!0,!0);m.attr("style",""),m.find(".label-suggestion-checkbox").click(function(){$(this).siblings('input[type="checkbox"]').click()}),getPadOuter().find(".comment-modal-comment").html("").append(m);const a=getPadOuter().find('iframe[name="ace_inner"]'),r=getPadOuter().find("#outerdocbody").outerWidth(!0),d=getPadOuter().find(".comment-modal").outerWidth(!0);let s=n.clientX,i=$(n.target).offset().top;t?(s+=a.offset().left,i+=parseInt(a.css("padding-top").split("px")[0]),i+=parseInt(padOuter.find("#outerdocbody").css("padding-top").split("px")[0])):s=$(n.target).offset().left-20,s+d>r&&(s=r-d-25);const p=t?t.outerHeight(!0):30;getPadOuter().find(".comment-modal").addClass("popup-show").css({left:`${s}px`,top:`${i+p}px`})}},adjustTopOf=(e,n)=>{const t=getPadOuter().find(`#${e}`);return t.css("top",`${n}px`),t},isOnTop=(e,n)=>{const t=getPadOuter().find(`#${e}`),o=`${n}px`;return t.css("top")===o},shouldNotCloseComment=e=>!!($(e.target).closest(".sidebar-comment").length||$(e.target).closest(".comment-modal").length);exports.hideComment=hideComment,exports.hideAllComments=hideAllComments,exports.highlightComment=highlightComment,exports.adjustTopOf=adjustTopOf,exports.isOnTop=isOnTop,exports.shouldNotCloseComment=shouldNotCloseComment;
